-weight: 800;\r\n    color: $blue;\r\n    gap: 8px;\r\n    max-height: 100px;\r\n}\r\n.like:hover, .dislike:hover, .show_edit:hover,\r\n.show_delete:hover, .show_reply:hover {\r\n    cursor: pointer;\r\n}\r\n.comment_main {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    .comment_text_area {\r\n        padding: 1rem;\r\n        color: $Grayish-Blue;\r\n    }\r\n    .comment_header {\r\n        display: flex;\r\n        justify-content: space-between;\r\n        padding-left: 1rem;\r\n\r\n        img {\r\n            width: 50px;\r\n            margin-top: 1rem;\r\n        }\r\n        .show_reply {\r\n            align-self: center;\r\n            margin-right: 2rem;\r\n            font-weight: 800;\r\n            font-size: 1.1rem;\r\n            color: $blue;\r\n        }\r\n        .comment_date {\r\n            color: $Grayish-Blue;\r\n        }\r\n        .comment_header1 {\r\n            display: flex;\r\n            align-items: center;\r\n            gap: 2rem;\r\n        }\r\n        .comment_name {\r\n            font-weight: 600;\r\n            color: $Dark-blue;\r\n            margin-left: -1rem;\r\n        }\r\n    }\r\n}\r\n.like, .dislike {\r\n    color: $Grayish-Blue;\r\n}\r\n.comment_you {\r\n    background-color: $blue;\r\n    color: $White;\r\n    border-radius: 5px;\r\n    font-size: 0.6rem;\r\n    padding: 5px;\r\n}\r\n.comment_bottom {\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n.show_on_large {\r\n    display: flex;\r\n}\r\n.show_delete {\r\n    color: $Red;\r\n    align-self: center;\r\n    margin-right: 1rem;\r\n    font-weight: 800;\r\n    font-size: 1.1rem;\r\n}\r\n.show_edit {\r\n    align-self: center;\r\n    margin-right: 2rem;\r\n    font-weight: 800;\r\n    font-size: 1.1rem;\r\n    color: $blue;\r\n}\r\n#form_container {\r\n    width: min(90%, 1140);\r\n    margin: 3rem auto;\r\n    background-color: $White;\r\n    padding: 2rem;\r\n    display: flex;\r\n    gap: 1rem;\r\n}\r\n.input_area {\r\n    width: 90%;\r\n}\r\n\r\n\r\n.show_reply.phone, .show_edit.phone, .show_delete.phone {\r\n    display: none;\r\n}\r\n@media only screen and (max-width: 600px) {\r\n    .comment_container {\r\n        grid-template-columns: 1fr;\r\n    }\r\n    .comment_side {\r\n        grid-row: 2;\r\n        display: flex;\r\n        flex-direction: row;\r\n        \r\n    }\r\n    .comment_bottom {\r\n    flex-direction: row;\r\n    }\r\n    .show_reply {\r\n        display: none;\r\n    }\r\n    .show_reply.phone, .show_edit.phone, .show_delete.phone {\r\n        display: block;\r\n    }\r\n    .show_reply.phone {\r\n        margin-left: 3rem;\r\n    }\r\n    .show_on_large {\r\n        display: none;\r\n    }\r\n    .comment_bottom {\r\n    margin-left: 2rem;\r\n    gap: 8px;\r\n    }\r\n    .comment_side.my_comment {\r\n        justify-content: space-between;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"80d353573d00a9b5d8fb7a1176b6f4d5","cacheData":{"env":{}}}